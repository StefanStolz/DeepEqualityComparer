<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net45</TargetFrameworks>
    <Version>2.0.2-alpha</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>StefanStolz</Authors>
    <Company />
    <PackageId>DeepEqualityComparer</PackageId>
    <Product />
    <Description>Generic IEqualityComparer which performs a deep comparison of object trees</Description>
    <Copyright>Copyright 2017 by Stefan Stolz</Copyright>
    <PackageLicenseUrl>https://raw.githubusercontent.com/StefanStolz/DeepEqualityComparer/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/StefanStolz/DeepEqualityComparer</PackageProjectUrl>
    <PackageTags>IEqualityComparer Comparer</PackageTags>
    <PackageReleaseNotes>.NET Standard support</PackageReleaseNotes>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(TargetFramework)' == 'netstandard2.0'">
    <DefineConstants>NETCORE;NETSTANDARD;NETSTANDARD2_0</DefineConstants>
  </PropertyGroup>
</Project>
